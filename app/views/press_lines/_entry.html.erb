<%= calendar_date_select_includes "custom", :locale=>'uk' %>
<p>
  <%= f.label :start %><br />
	<%= f.calendar_date_select :start, {:month_year => :label, :minute_interval => 5} %>
</p>
<p>
  <%= f.label :display_title %><br />
  <%= f.text_field :display_title %>
</p>
<p>
  <%= f.label :original_title %><br />
  <%= f.text_field :original_title %>
</p>
<p>
  <%= f.label :channel_id %><br />
  <%= f.collection_select (:channel_id, @channels, :id, :name, {:prompt => true})   %>
</p>
<p>
  <%= f.label :lead_text %><br />
  <%= f.text_field :lead_text %>
</p>
<p>
  <%= f.label :priority %>
	<%= f.check_box :priority %><br />
</p>