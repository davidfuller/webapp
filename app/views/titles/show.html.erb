<p>
  <b>Schedule Title:</b>
  <%=h @title.schedule_title %>
</p>

<p>
  <b>Display Title:</b>
  <%=h @title.display_title %>
</p>

<table>
	<tr>
		<td>House Number</td>
		<td>Series Number</td>
	</tr>
	<% @title.houses.each do |h| %>
		<tr>
			<td><%=h h.house_number %></td>
			<td><%=h h.series_number.series_number if h.series_number %>
		</tr>
	<% end %>
</table>

<%= link_to 'Edit', edit_title_path(@title) %> |
<%= link_to 'Back', :back %>

