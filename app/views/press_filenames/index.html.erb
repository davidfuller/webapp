<h1>Listing Press Filenames</h1>

<table>
  <tr>
    <th>Filename</th>
    <th>Import date</th>
  </tr>

<% @press_filenames.each do |press_filename| %>
  <tr>
    <td><%=h press_filename.filename %></td>
    <td><%=h press_filename.import_date %></td>
    <td><%= link_to 'Show', press_filename %></td>
    <td><%= link_to 'Edit', edit_press_filename_path(press_filename) %></td>
    <td><%= link_to 'Destroy', press_filename, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New press_filename', new_press_filename_path %>